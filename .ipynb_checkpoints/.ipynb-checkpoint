    "cur.execute(\"\"\"SELECT HomeTeam, SUM(FTHG) AS TotalHG, SUM(FTAG) AS TotalHAG\n",
    "                FROM matches\n",
    "                WHERE Season == 2011\n",
    "                GROUP BY HomeTeam;\"\"\")\n",
    "df_Home_2011 = pd.DataFrame(cur.fetchall())\n",
    "df_Home_2011.columns = [x[0] for x in cur.description]\n",
    "df_Home_2011.head()"
   ]
